<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Demo Authors -->
    <record id="author_1" model="library.author">
        <field name="name">J.K. Rowling</field>
        <field name="age">58</field>
    </record>

    <record id="author_2" model="library.author">
        <field name="name">Stephen King</field>
        <field name="age">76</field>
    </record>

    <!-- Demo Books -->
    <record id="book_1" model="library.book">
        <field name="name">Harry Potter and the Philosopher's Stone</field>
        <field name="author_id" ref="author_1"/>
        <field name="published_date">1997-06-26</field>
    </record>

    <record id="book_2" model="library.book">
        <field name="name">The Shining</field>
        <field name="author_id" ref="author_2"/>
        <field name="published_date">1977-01-28</field>
    </record>
</odoo>